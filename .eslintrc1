{
    "parser": "babel-eslint",

    "env": {
        "browser": true,
        "node": true
    },

    "globals": {
        "React": true
    },

    "ecmaFeatures": {
        "jsx": true
    },

    "plugins": [
        "react"
    ],
    "rules": {
        "max-len": [1, 120, 2, {
            "ignoreComments": true
        }],
        "globals": 0,
        "prop-types": 0
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"]
}